{
    "rewrites": [
          {
                  "source": "/",
                  "destination": "/docs.html"
          },
          {
                  "source": "/api/v1/match",
                  "destination": "/api/v1?route=match"
          },
          {
                  "source": "/api/v1/reputation/:network/:agentId",
                  "destination": "/api/v1?route=reputation&network=:network&agentId=:agentId"
          },
          {
                  "source": "/api/admin/health",
                  "destination": "/api/admin/stats?type=health"
          },
          {
                  "source": "/api/x402",
                  "destination": "/api/x402"
          },
          {
                  "source": "/api/x402/tokens/price",
                  "destination": "/api/x402/tokens/price"
          },
          {
                  "source": "/api/x402/tokens/prices/multichain",
                  "destination": "/api/x402/tokens/prices/multichain"
          },
          {
                  "source": "/api/x402/pools/analytics",
                  "destination": "/api/x402/pools/analytics"
          },
          {
                  "source": "/api/x402/transactions/whales",
                  "destination": "/api/x402/transactions/whales"
          },
          {
                  "source": "/api/x402/contracts/safety",
                  "destination": "/api/x402/contracts/safety"
          },
          {
                  "source": "/api/x402/token-price",
                  "destination": "/api/x402/tokens/price"
          },
          {
                  "source": "/api/x402/multichain-price",
                  "destination": "/api/x402/tokens/prices/multichain"
          },
          {
                  "source": "/api/x402/pool-analytics",
                  "destination": "/api/x402/pools/analytics"
          },
          {
                  "source": "/api/x402/whale-transactions",
                  "destination": "/api/x402/transactions/whales"
          },
          {
                  "source": "/api/x402/contract-safety",
                  "destination": "/api/x402/contracts/safety"
          },
          {
                  "source": "/docs",
                  "destination": "/docs.html"
          },
          {
                  "source": "/.well-known/x402.json",
                  "destination": "/public/.well-known/x402.json"
          }
    ],
    "headers": [
          {
                  "source": "/.well-known/x402.json",
                  "headers": [
                            {
                                        "key": "Content-Type",
                                        "value": "application/json"
                            },
                            {
                                        "key": "Access-Control-Allow-Origin",
                                        "value": "*"
                            }
                  ]
          }
    ]
}
