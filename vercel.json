{
  "rewrites": [
    {
      "source": "/",
      "destination": "/docs.html"
    },
    {
      "source": "/api/x402/token-price",
      "destination": "/api/x402/token-price"
    },
    {
      "source": "/api/x402/multichain-price",
      "destination": "/api/x402/multichain-price"
    },
    {
      "source": "/api/x402/pool-analytics",
      "destination": "/api/x402/pool-analytics"
    },
    {
      "source": "/api/x402/whale-transactions",
      "destination": "/api/x402/whale-transactions"
    },
    {
      "source": "/api/x402/contract-safety",
      "destination": "/api/x402/contract-safety"
    },
    {
      "source": "/docs",
      "destination": "/docs.html"
    },
    {
      "source": "/.well-known/x402.json",
      "destination": "/public/.well-known/x402.json"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/x402.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
